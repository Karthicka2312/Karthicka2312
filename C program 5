//Array Using for loop
#include<stdio.h>
void main()
{
    int n;
    printf("Enter number of value for array : ");
    scanf("%d",&n);
    int age[n];
    for(int i=0;i<n;i=i+1)
    {
        
        printf("Enter value 1");
        
        scanf("%d",&age[i]);
    }
    for(int i=0;i<n;i=i+1)
    {
    printf("/n Value in index %d is %d",i,age[i]);
    }
}
